<%- include("partials/facHeader") -%> 

<section style="padding-top: 50px;">
    <div class="container py-5">
      
  
      <div class="row">
        
        <div class="col-lg-8">
          <div class="card mb-4">
            <div class="card-body">
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Full Name</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= name %></p>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Email</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= username %></p>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Designation</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= designation %></p>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Branch</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= dept %> - B.tech</p>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Mobile</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= phone %></p>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Scores</p>
                </div>
                <div class="col-sm-9">
                  <p class="text-muted mb-0"><%= scores %></p>
                </div>
              </div>
              
            </div>
          </div>
         
        </div>
        <div class="col-lg-4">
          <div class="card mb-4">
            <div class="card-body text-center">
              <img src="profImage.png" alt="avatar"
                class="rounded-circle img-fluid" style="width: 150px;">
              <h5 class="my-3"><%= name %></h5>
              <p class="text-muted mb-0"><%= designation %></p>
              <p class="text-muted mb-3"><%= dept %></p>
              <button class="btn-upvote"><a href = '/'>Logout</a></button>
              
            </div>
          </div>
         
        </div>
      </div>
    </div>
  </section>
<%- include("partials/footer") -%>